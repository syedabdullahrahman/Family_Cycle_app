<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="10dp"
    android:paddingLeft="10dp"
    android:paddingRight="3dp"
    tools:context="com.example.user.familycyclefinal.Married_Couple_Profile_Activity">
    
        
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="800dp">

            <TextView
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:text="    My Profile"
                android:textColor="#196F3D"
                android:textStyle="bold"
                android:textAlignment="center"
                android:textSize="32dp"
                android:id="@+id/mypro" />

            <ImageView
                android:id="@+id/profileedit"
                android:layout_width="80dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/mypro"
                android:layout_toRightOf="@+id/mypro"
                android:src="@drawable/proedit" />

            <ImageView
                android:id="@+id/nijerpic"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_below="@+id/profileedit"
                android:layout_marginEnd="45dp"
                android:layout_marginRight="45dp"
                android:layout_marginTop="38dp"
                android:layout_toLeftOf="@+id/imageView3"
                android:layout_toStartOf="@+id/imageView3"
                app:srcCompat="@drawable/jamai" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignTop="@+id/nijerpic"
                android:layout_toLeftOf="@+id/profileedit"
                android:layout_toStartOf="@+id/profileedit"
                app:srcCompat="@drawable/bou" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/nijerpic"
                android:layout_marginTop="62dp"
                android:text="Name :"
                android:textColor="@android:color/holo_red_dark"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/myname"
                android:layout_width="220dp"
                android:layout_height="50dp"
                android:layout_alignBaseline="@+id/textView9"
                android:layout_alignBottom="@+id/textView9"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/textView9"
                android:layout_toRightOf="@+id/textView9"
                android:text="TextView" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textView9"
                android:text="Spouse Name:"
                android:textColor="@android:color/holo_red_dark"
                android:textSize="15dp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/spousename"
                android:layout_width="220dp"
                android:layout_height="50dp"
                android:layout_alignBaseline="@+id/textView10"
                android:layout_alignBottom="@+id/textView10"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/textView10"
                android:layout_toRightOf="@+id/textView10"
                android:text="TextView" />
            <TextView
                android:id="@+id/textView11"
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textView10"
                android:text="My date of birth:"
                android:textColor="@android:color/holo_red_dark"
                android:textSize="15dp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/mydob"
                android:layout_width="220dp"
                android:layout_height="50dp"
                android:layout_alignBaseline="@+id/textView11"
                android:layout_alignBottom="@+id/textView11"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/textView11"
                android:layout_toRightOf="@+id/textView11"
                android:text="TextView" />
            <TextView
                android:id="@+id/textView12"
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textView11"
                android:text="Spouse of birth:"
                android:textColor="@android:color/holo_red_dark"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/spousedob"
                android:layout_width="220dp"
                android:layout_height="50dp"
                android:layout_alignBaseline="@+id/textView12"
                android:layout_alignBottom="@+id/textView12"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/textView12"
                android:layout_toRightOf="@+id/textView12"
                android:text="TextView" />
            <TextView
                android:id="@+id/textView13"
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textView12"
                android:text="Date of Marriage : "
                android:textColor="@android:color/holo_red_dark"
                android:textSize="15dp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/marriagedate"
                android:layout_width="220dp"
                android:layout_height="50dp"
                android:layout_alignBaseline="@+id/textView13"
                android:layout_alignBottom="@+id/textView13"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/textView13"
                android:layout_toRightOf="@+id/textView13"
                android:text="TextView" />
            <TextView
                android:id="@+id/textView14"
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textView13"
                android:text="Email Address : "
                android:textColor="@android:color/holo_red_dark"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/emailaddress"
                android:layout_width="220dp"
                android:layout_height="50dp"
                android:layout_alignBaseline="@+id/textView14"
                android:layout_alignBottom="@+id/textView14"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/textView14"
                android:layout_toRightOf="@+id/textView14"
                android:text="TextView" />


        </RelativeLayout>

    
    
</ScrollView>
